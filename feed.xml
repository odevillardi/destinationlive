---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    {% assign lives = site.lives | sort %}
    {% for live in lives reversed %}
      <item>
        <title>{{ live.qui | url_decode | strip_html | strip_newlines }} → {{ live.ou | url_decode | strip_html | strip_newlines }}</title>
        <description>
          Nouveau live proposé de {{ live.qui | url_decode | strip_html | strip_newlines }} à {{ live.ou | url_decode | strip_html | strip_newlines }}<br>
          <iframe class="embed-responsive-16by9" src="https://www.youtube-nocookie.com/embed/{{ live.videoid }}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          <noscript>Voir la vidéo de <a href="https://www.youtube.com/watch?v={{ live.videoid }}">{{ page.qui | url_decode }} sur Youtube</a></noscript>
        </description>
        <pubDate>{{ live.created_at | date_to_rfc822 }}</pubDate>
        <link>https://destinationlive.net/merci.html?artist={{ live.qui | strip_html | strip_newlines | url_encode }}&amp;place={{ live.ou | strip_html | strip_newlines | encode_url }}&amp;liveid={{ live.liveid | strip_html | strip_newlines | url_encode }}&amp;videoid={{ live.videoid | strip_html | strip_newlines | url_encode }}</link>
        <guid isPermaLink="true">https://destinationlive.net/merci.html?artist={{ live.qui | strip_html | strip_newlines | url_encode }}&amp;place={{ live.ou | strip_html | strip_newlines | encode_url }}&amp;liveid={{ live.liveid | strip_html | strip_newlines | url_encode }}&amp;videoid={{ live.videoid | strip_html | strip_newlines | url_encode }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
